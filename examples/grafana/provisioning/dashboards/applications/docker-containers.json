        ]
      },
      "pluginVersion": "9.0.0",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "expr": "increase(container_start_time_seconds{name!~\"POD|\",name=~\"$container\"}[24h])",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Container Restarts (Last 24h)",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "id": true,
              "image": false,
              "instance": true,
              "job": true
            },
            "indexByName": {
              "Value": 2,
              "image": 1,
              "name": 0
            },
            "renameByName": {
              "Value": "Restarts (24h)",
              "image": "Image",
              "name": "Container"
            }
          }
        }
      ],
      "type": "table"
